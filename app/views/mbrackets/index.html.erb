<p id="notice"><%= notice %></p>

<h1>Listing Mbrackets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mbrackets.each do |mbracket| %>
      <tr>
        <td><%= mbracket.name %></td>
        <td><%= link_to 'Show', mbracket %></td>
        <td><%= link_to 'Edit', edit_mbracket_path(mbracket) %></td>
        <td><%= link_to 'Destroy', mbracket, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mbracket', new_mbracket_path %>
